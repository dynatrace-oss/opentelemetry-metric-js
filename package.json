    "@typescript-eslint/parser": "^4.28.3",
    "eslint": "^7.30.0",
    "eslint-plugin-deprecation": "^1.2.1",
    "eslint-plugin-header": "^3.1.1",
    "jest": "^27.0.6",
    "markdownlint-cli": "^0.27.1",
    "mock-fs": "^5.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.3",
    "typescript": "^3.9.10"
