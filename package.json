    "@opentelemetry/metrics": "^0.23.0"
  },
  "peerDependencies": {
    "@opentelemetry/api": "1.0.1"
  },
  "devDependencies": {
    "@opentelemetry/api": "1.0.1",
    "@opentelemetry/core": "^0.23.0",
    "@opentelemetry/metrics": "^0.23.0",
    "@types/jest": "^26.0.24",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^16.3.2",
